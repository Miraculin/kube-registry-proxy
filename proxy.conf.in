global
  maxconn 1024

defaults
  mode http
  retries 3
  option redispatch
  timeout client 3600s
  timeout server 3600s
  timeout connect 5s

frontend forwarder
  bind *:%FWDPORT%
  default_backend registry

backend registry
  server kube-registry %HOST%:%PORT% ssl verify required ca-file %CA_FILE%
